=TEXTJOIN(",", TRUE, IF(ISNUMBER(SEARCH(TEXT(SEQUENCE(3, 1, DATE(2022, 12, 1), 1), "yyyy-mm-dd"), E5 & ",")), MID(E5 & ",", SEARCH(TEXT(SEQUENCE(3, 1, DATE(2022, 12, 1), 1), "yyyy-mm-dd"), E5 & ","), SEARCH(",", E5 & ",", SEARCH(TEXT(SEQUENCE(3, 1, DATE(2022, 12, 1), 1), "yyyy-mm-dd"), E5 & ","))) - 1), TEXT(SEQUENCE(3, 1, DATE(2022, 12, 1), 1), "yyyy-mm-dd") & ":0"))
