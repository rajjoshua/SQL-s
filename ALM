SELECT
    TC.TST_ID,
    TC.TST_NAME,
    TC.TST_FOLDER,
    TC.TST_STATUS,
    DEFECT.FLD_ID,
    DEFECT.FLD_BUG_ID,
    DEFECT.FLD_STATUS
FROM
    TEST_CASE TC
LEFT OUTER JOIN
    TEST_CASE_DEFECT_LINK TCDL ON TC.TST_ID = TCDL.TST_ID
LEFT OUTER JOIN
    DEFECT ON TCDL.FLD_ID = DEFECT.FLD_ID
WHERE
    TC.TST_FOLDER = 'YourSpecificFolderName'
    AND TC.TST_CYCL_ID = 'YourSpecificCycleID'
